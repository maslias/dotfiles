aliases account "`pass meta email/bodycheck user`"  
host  "`pass meta email/bodycheck host`"
port "`pass meta email/bodycheck port_msmtp`" 
from "`pass meta email/bodycheck user`"  
user "`pass meta email/bodycheck user`"  
passwordeval "`pass email/bodycheck`"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile "`$XDG_STATE_HOME`/msmtp/msmtp.log"
tls_starttls off
